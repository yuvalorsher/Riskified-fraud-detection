{
  "data_prep_params": {
    "data_loader_params": {
      "data_type": "csv",
      "data_path": "Data/transactions.csv",
      "additional_load_params": {
        "parse_dates": ["created_at"]
      }
    },
    "dataset_sampler_params": {
      "sampler_type": "random_sampler",
      "additional_sampler_params": {
        "n": null,
        "frac": 0.8
      }
    },
    "clear_label_params": {
      "labels_to_clear": ["declined"]
    },
    "drop_na_rows_params": {
      "columns_to_dropna": []
    },
    "clipper_params": {
      "keep_original_col": 0,
      "columns_to_clip": {
        "V4_our_age": {
        "lower": 0,
        "upper": 100
      },
      "V5_merchant_age": {
        "lower": 0,
        "upper": 100
      }
      }
    }
  },
  "train_params": {
    "train_test_split_params": {
      "train_start_date": "2016-09-01",
      "test_start_date": "2017-04-01",
      "test_end_date": "2017-06-01"
    },
    "trainset_sampler_params": {
      "sampler_type": "random_over_sampler",
      "additional_sampler_params": {
        "label_col": "status",
        "positive_label": "chargeback",
        "positive_to_negative_ratio": 0.5
      }
    },
    "features_params": {
      "one_hot_params": {
        "columns_to_onehot": [
          "billing_country_code",
          "shipping_country_code",
          "shipping_method",
          "currency_code",
          "gateway",
          "V4_our_age",
          "V5_merchant_age",
          "V1_link",
          "V6_avs_result",
          "V7_bill_ship_name_match"
        ]
      },
      "drop_columns": {
        "columns_names": [
          "order_id",
          "status",
          "email_anoni",
          "created_at"
        ]
      }
    },
    "target_params": {
      "target_mapping": {
        "target_col": "status",
        "target_mapping_dict": {
          "chargeback": 1,
          "approved": 0
        }
      }
    },
    "classifier_params": {
      "classifier_type": "xgb",
      "additional_model_params": {
        "model_initialize_params": {
          "tree_method": "hist",
          "early_stopping_rounds": 2
        },
        "model_fit_params": {

        },
        "required_approval_rate": 0.9
      }
    }
  },
  "evaluation_params": {
    "cost_of_cb_to_revenue_ratio": 0.5
  }
}